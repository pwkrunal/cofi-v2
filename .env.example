# Cofi Services - Root Level Environment Configuration
# Copy this file to .env and update with your actual values
# These are shared settings for docker-compose.yml

# Storage Paths
STORAGE_PATH=./audio_files
CLIENT_VOLUME=./data

# MySQL Configuration (shared by all services)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=password
MYSQL_DATABASE=testDb

# SSE Configuration for Dashboard
SSE_POLL_INTERVAL=5
